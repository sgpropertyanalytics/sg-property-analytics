{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://sg-property-analyzer.onrender.com/api/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}



