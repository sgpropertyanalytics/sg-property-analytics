{
  "name": "sg-property-dashboard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && npm run typecheck && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "playwright test e2e/smoke.spec.js e2e/boot-hydration.spec.js e2e/rapid-interaction.spec.js",
    "test:e2e:full": "playwright test",
    "test:e2e:smoke": "playwright test e2e/smoke.spec.js",
    "test:perf": "playwright test e2e/performance/",
    "test:perf:headed": "playwright test e2e/performance/ --headed",
    "test:perf:filters": "playwright test e2e/performance/all-filters.spec.js",
    "test:perf:nav": "playwright test e2e/performance/navigation.spec.js e2e/performance/cross-page.spec.js",
    "test:perf:stress": "playwright test e2e/performance/stress-test.spec.js e2e/performance/filter-combinations.spec.js",
    "test:perf:quick": "playwright test e2e/performance/page-load.spec.js",
    "test:perf:report": "playwright test e2e/performance/ --reporter=html && open playwright-report/index.html",
    "test:perf:diagnose": "playwright test e2e/performance/bottleneck-diagnostic.spec.js",
    "test:perf:charts": "playwright test e2e/performance/per-chart-timing.spec.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:ci": "eslint \"src/**/*.{js,jsx,ts,tsx}\" && node scripts/design-guard.js",
    "lint:design": "node scripts/design-guard.js",
    "lint:strict": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@fontsource/geist-sans": "^5.2.5",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/playfair-display": "^5.2.8",
    "@fontsource/space-grotesk": "^5.2.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "autoprefixer": "^10.4.23",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "firebase": "^12.7.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.561.0",
    "maplibre-gl": "^4.7.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^10.0.0",
    "react-map-gl": "^7.1.9",
    "react-router-dom": "^6.20.0",
    "use-debounce": "^10.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^5.0.8",
    "vitest": "^4.0.16"
  }
}
